				
body {
    background-color: #9FB6CD;
	text-align:center; 
	font-family: arial;
	text-align:center; 
	font-family: Tahoma,Verdana, Helvetica, Arial;
    font-size: 12px;
}	

canvas { background-color: #d2e9ff; }

  hr {
  	margin: 2em 0;
  	color: #C7C7C7;
  	background: #C7C7C7;
  	border-color: #C7C7C7;
  	border-style: none;
  	height: 1px;
  }
  li {
  	margin: 1em 0;
  }
  td {
  	vertical-align: top;
  	padding: 0.5em;
  }
  th {
  	text-align: left;
  	color: #fff;
  	padding: 0.5em;
  }
  ol, ul {
  	margin: 2em 0;
  	padding-left: 1em;
  }

.box { 
    background-color: #f0f5fb;
	width: 800px;
	text-align:left;
	margin:5px auto;
 }
 
.boxtop div {
    background: url(top.jpg);
 }
 
.boxtop { 
    background: url(top.jpg);
 }

.boxbottom div {
    background: url(bottom.jpg);
 }
 
.boxbottom {
    background: url(bottom.jpg);
 }

.boxtop div, .boxtop, .boxbottom div, .boxbottom {
	width: 100%;
	height: 25px;
	font-size: 25px;
}

.content 
{
	width:800px;
	 text-align:center
	margin 0 auto;
 }

.header
{
	margin-left:8px;
	margin-right:8px;
	background: url(main.jpg);
	height:120px;
}

.navigation
{
	margin:20px 0px 20px 0px;
	font-size: 1em;
}

.navigation a:link {
	color: #000000;
	text-decoration: none;
}

.navigation a:visited {
	color: #000000;
	text-decoration: none;
}

.navigation a:hover{
	color: #f0f5fb;
	text-decoration: none;
}

.navigation a:active {
	color: #000000;
	text-decoration: none;
}


.main {
	width:800px;
	padding-top:5px;
	font-size: 1em;
}

.main h1 {
	font-size: 1.5em; 
}
	
.footer {
	font-size: .8em; 
	text-align:center; 
	width:800px;
}

.clear {
	float: none;
	clear: both;
	height: 1px;
	margin: 0;
	padding: 0;
}
